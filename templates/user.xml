<templateSet group="user">
  <template name="bdp" value="binding.pry" description="rails 调试" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="ymd" value="&quot;%Y-%m-%d %H:%M:%S&quot;" description="strftime" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="cl" value="console.log(&quot;&quot;)" description="console.log()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="rli" value="Rails.logger.info(&quot;&quot;)" description="rails 日志输出" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="rlw" value="Rails.looger.warn(&quot;&quot;)" description="rails 日志" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="rld" value="Rails.logger.debug(&quot;&quot;)" description="rails 日志输出" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
</templateSet>